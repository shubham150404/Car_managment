                      <div class="row">

                        <div class="form-group col-6">
                          <label for="exampleInputEmail1 text-white">Car Name</label>
                          <input type="text" class="form-control" name="car_name" autocomplete="off"
                            id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Car Name">
                          <br>
                        </div>


                        <div class="form-group col-6">
                          <label for="monthInput">Registration Year</label>
                          <input type="month" class="form-control" name="reg_year" autocomplete="off" id="monthInput"
